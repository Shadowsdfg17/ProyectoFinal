<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.andrea.proyectofinal.controller.MenubdController">
    <children>
        <Label text="BIENVENIDO">
         <font>
            <Font name="Century Gothic Bold" size="22.0" />
         </font></Label>
      <Label text="INICIA SESIÓN COMO ADMINISTRADOR O COMO INVITADO PARA CONTINUAR" />
        <Button mnemonicParsing="false" onAction="#verArtista" prefHeight="25.0" prefWidth="225.0" text="INICIAR SESIÓN COMO INVITADO">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <Button mnemonicParsing="false" onAction="#iniciaSesion2" text="INICIAR SESIÓN COMO ADMINISTRADOR">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
        <Button fx:id="secondaryButton" onAction="#retrocederVentana" prefHeight="25.0" prefWidth="138.0" text="VOLVER A CONECTAR" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
